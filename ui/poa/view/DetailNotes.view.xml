<core:View
	controllerName="sap.shineNext.poa.view.DetailNotes"
	xmlns="sap.m"
	xmlns:commons="sap.ui.commons"
	xmlns:form="sap.ui.commons.form"
	xmlns:layout="sap.ui.commons.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core" >
	<List
		id="list"
		showSeparators="None"
		items="{path:'Notes'}" >
		<FeedListItem
			sender="{Author}"
			icon="{Thumbnail}"
			timestamp="{path:'CreatedAt', formatter:'sap.shineNext.poa.util.Formatter.Date'}"
			text="{Text}" />
	</List>
</core:View>